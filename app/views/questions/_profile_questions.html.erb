<div class="questions_any">
<% if users_questions.any? %>
  <h3>Your Questions</h3>
<% end %>

<ul class="profile_questions">
  <% @users_questions.each do |question| %>
    <li><%= link_to question.title, question %> 
  <%= link_to "Edit", edit_question_path(question) %>
  <%= link_to "Delete", question_path(question), :method => :delete %></li>
 <br>


  <% end %>
</ul>
