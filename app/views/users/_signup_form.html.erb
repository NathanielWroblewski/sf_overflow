<div class="signup_form">
  <%= form_for @user do |form| %>
    <%= form.label :username %><br>
    <%= h form.text_field :username %><br>
    <%= form.label :email %><br>
    <%= h form.text_field :email %><br>
    <%= form.label :password %><br>
    <%= h form.password_field :password %><br>
    <%= form.submit %>
  <% end %>
</div>
