<div class="signup_form">
  <%= form_for @user do |form| %>
    <%= form.label :username %>
    <%= h form.text_field :username %>
    <%= form.label :email %>
    <%= h form.text_field :email %>
    <%= form.label :password %>
    <%= h form.password_field :password %>
    <%= form.submit %>
  <% end %>
</div>
